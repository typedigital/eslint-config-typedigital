{
  "name": "@typedigital/eslint-config",
  "version": "4.0.0",
  "description": "typedigital JavaScript style guide",
  "main": "base.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/typedigital/eslint-config-typedigital"
  },
  "keywords": [
    "eslint",
    "coding",
    "rules",
    "style",
    "guide",
    "typedigital",
